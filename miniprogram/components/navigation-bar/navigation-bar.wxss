.navigation-bar {
  display: flex;
  justify-content: space-around;
  background-color: #f0f0f0;
  padding: 5px;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}

.navigation-bar-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 16px;
  color: #333;
}

.navigation-bar-icon {
  width: 20px;
  height: 20px;
  margin-bottom: 5px;
}
